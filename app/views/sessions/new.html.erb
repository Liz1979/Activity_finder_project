<% provide(:title, "Login") %>
<div class = form>
   <div class="text-div">
      <h1>Login</h1>
      <%= form_tag '/login' do %>

      <p>UserName:</br> <%= text_field_tag :username %></br></p>
      <p>Password:</br> <%= password_field_tag :password %></br></p>
      <p></p>
      <p><%= submit_tag "Submit", class: "button"%></p>
      <p></p>
      </br>
      <% end %>
   </div>
</div>